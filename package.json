    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "cross-env": "^7.0.3",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@^6.3.19",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  },
  "trustedDependencies": [
    "@biomejs/biome"
